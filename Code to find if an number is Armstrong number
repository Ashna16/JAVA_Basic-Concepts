class Program25
{ public static void main (String args[])
{ int i,q,r,sum,flag;
  i=Integer.parseInt(args[0]);
  sum=0;
  flag=i;
do
{
  r= i%10;
  q= i/10;
  i=q;
  sum=sum+ (r*r*r);
} while(i!=0);
if (flag = = sum)
{
System.out.println("it is an Amstrong number");
}
else
{ System.out.println("it is not an Amstrong number");
}
}
}
