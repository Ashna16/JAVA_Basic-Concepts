import java.util.Scanner
class program45
{
public static void main(String args[])
{
Scanner s1=new Scanner(System.in);
int[][] no1= new int[3][3];
for(r=0 ; r<=1; r=r+1)
{
for(c=0;c<=1;c=c+1)
{
System.out.println("Enter no.");
no1[r][c]=s1.nextInt();
}
}
for( ; ; )
{
for(; ; )
{
no1[2][0]=no1[0][0] + no1[1][0];
no1[2][1]=no1[0][1] + no1[1][1];
no1[2][2]=no1[0][2] + no1[1][2];
}
}
for(r=0;r<=2;r=r+1)
{
for(c=0;c<=2;c=c+1)
{
System.out.print(" " +no1[r][c]);
}
System.out.print("\n");
}
}
}

