class Prrogram11
{ 
public static void main (String args[]);
{ int n1,n2;
 n1=Integer.parseInt(args[0]);
 n2=Integer.parseInt(args[1]);
 if (n1% 2 == 0 && n2% 2 ==0)
 { System.out.println ("n1 and n2 are even numbers");
 }
 else if (n1%2 ==0 && n2%2 != 0)
 { System.out.println("n1 is even and n2 is odd");
 }
 else if (n1%2 != 0 && n2%2 ==0)
{
System.out.println("n1 is odd and n2 is even");
}
}
}
