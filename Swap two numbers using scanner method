import.java.util.Scanner;
{ class Program5
   public static void main (String[] args);
   { Scanner s1= new Scanner(System.in);
    int a,b,temp;
    System.out.println("enter a");
    a=s1.nextInt();
    System.out.println("enter b");
    b=s1.nextInt();
    temp=a;
    a=b;
    b=temp;
    System.out.println("a is " + a);
    System.out.println("b is " + b);
   }
   }
