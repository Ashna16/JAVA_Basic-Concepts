class Program34
{ public static void main(String args[0])
{ int i,n,c;
n=Integer.parseInt(args[0]);
c=0;
while(i<n)
{ 
i=i+1;
if (n%i != 0)
{ c=c+0;
}
else
{ c= c+1;
}
}
if (c = =0)
{ System.out.println("Number is a prime number");
}
else
{ System.out.println("number is not a prime number");
}
}
} 
